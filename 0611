import java.util.*;
public class Main{
    public static void main(String[] args){
        Scanner sc = new Scanner(System.in);
        int[] scores = new int[5];
        int i =0;
        while(i<5){
            scores[i] = sc.nextInt();
            System.out.println("scores["+i+"]= "+scores[i]);
            i = i+1;
        }
    }
}
